{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myadfbigdatapractise"
		},
		"LS_AzureDataLakeStorage_ADLS_OUTPUT_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage_ADLS_OUTPUT'"
		},
		"LS_AzureDataLakeStorage_TO_ADLS_OUTPUT_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage_TO_ADLS_OUTPUT'"
		},
		"LS_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSqlDatabase'"
		},
		"LS_AzureSqlDatabase_To_ADLS_INPUT_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSqlDatabase_To_ADLS_INPUT'"
		},
		"LS_AzureDataLakeStorage_ADLS_OUTPUT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsmaya.dfs.core.windows.net/"
		},
		"LS_AzureDataLakeStorage_TO_ADLS_OUTPUT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsmaya.dfs.core.windows.net/"
		},
		"LS_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mayadevser.database.windows.net"
		},
		"LS_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mayadbsqlp"
		},
		"LS_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mayaadmin"
		},
		"LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mayadevser.database.windows.net"
		},
		"LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mayadbsqlp"
		},
		"LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mayaadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "myadfbigdatapractise_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorage_ADLS_OUTPUT_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_ADLS_OUTPUT_accountKey')]"
					},
					"LS_AzureDataLakeStorage_TO_ADLS_OUTPUT_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_TO_ADLS_OUTPUT_accountKey')]"
					},
					"LS_AzureSqlDatabase_password": {
						"value": "[parameters('LS_AzureSqlDatabase_password')]"
					},
					"LS_AzureSqlDatabase_To_ADLS_INPUT_password": {
						"value": "[parameters('LS_AzureSqlDatabase_To_ADLS_INPUT_password')]"
					},
					"LS_AzureDataLakeStorage_ADLS_OUTPUT_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_ADLS_OUTPUT_properties_typeProperties_url')]"
					},
					"LS_AzureDataLakeStorage_TO_ADLS_OUTPUT_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_TO_ADLS_OUTPUT_properties_typeProperties_url')]"
					},
					"LS_AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"LS_AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"LS_AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_server')]"
					},
					"LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_database')]"
					},
					"LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSqlDatabase_To_ADLS_INPUT_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}